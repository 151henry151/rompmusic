# RompMusic - Docker Compose environment
# Copy to .env and fill in values

# Required: generate a random secret (e.g. openssl rand -hex 32)
JWT_SECRET=your-jwt-secret-here

# Optional: override defaults
DB_PASSWORD=rompmusic
MUSIC_PATH=./music
ADMIN_COOKIE_SECRET=your-admin-cookie-secret

# Recommendations: Last.fm API key (optional; free at last.fm/api/account/create)
LASTFM_API_KEY=

# Email (optional; for password reset, verification)
# Example for Spacemail: SMTP_HOST=mail.spacemail.com SMTP_PORT=587
SMTP_HOST=
SMTP_USER=
SMTP_PASSWORD=
SMTP_FROM=
